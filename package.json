{
	"permissions": "*",
	"routing": "routing.json",
	"frontend":"frontend",
	"autoload": {
		"files":[
			{
				"file":"libraries/base/url.php",
				"function": true
			}
		],
		"directories": ["controllers", "libraries", "Listeners", "logs", "validators", "views", "processes"]
	},
	"languages":{
		"fa_IR": "langs/fa_IR.json",
		"en_US": "langs/en_US.json"
	},
	"events": [
		{
			"name":"packages/userpanel/Events/UserTypePermissionsList",
			"listener": "packages/userpanel/Listeners/Settings/UserType@permissions_list"
		},
		{
			"name":"packages/userpanel/Events/search",
			"listener": "packages/userpanel/Listeners/Search@find"
		},
		{
			"name":"packages/userpanel/Events/Users/BeforeDelete",
			"listener": "packages/userpanel/Listeners/Users/BeforeDelete@check"
		},
		{
			"name":"packages/userpanel/Events/AuthenticationHandlersEvent",
			"listener": "packages/userpanel/Listeners/AuthenticationHandlers@add"
		},
		{
			"name": "packages/notifications/Events",
			"listener": "packages/userpanel/Listeners/Notifications@events"
		},
		{
			"name":"packages/notice/Events/Views",
			"listener": "packages/userpanel/Listeners/Notice@views"
		},
		{
			"name":"packages/userpanel/Events/Settings",
			"listener": "packages/userpanel/Listeners/Users/Settings@settingsList"
		},
		{
			"name":"packages/userpanel/Events/General/Settings",
			"listener": "packages/userpanel/Listeners/Settings@init"
		}
	]
}
